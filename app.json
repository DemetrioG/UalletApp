{
  "expo": {
    "name": "Uallet",
    "slug": "Uallet",
    "scheme": "uallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-facebook",
        {
          "facebookScheme" : "fb623678532217571",
          "facebookAppId" : "623678532217571",
          "facebookDisplayName" : "Uallet"
        }
      ]
    ]
    
  }
}
