{
  "expo": {
    "name": "Uallet",
    "slug": "uallet",
    "scheme": "uallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/src/icon.png",
    "splash": {
      "image": "./assets/images/src/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "bundleIdentifier": "com.uallet.uallet",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/src/adaptive-icon.png",
        "backgroundColor": "#266DD3"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.uallet.uallet",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/src/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-facebook",
        {
          "facebookScheme": "fb623678532217571",
          "facebookAppId": "623678532217571",
          "facebookDisplayName": "Uallet"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/src/icon.png",
          "color": "#FFFFFF"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "87d0b7d4-bbd1-44ae-bc18-478c2aecb7ec"
      }
    }
  }
}
